<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.flm.entity.Admin" table="admin">
		<id name="adminId" column="admin_id">
			<generator class="increment" />
		</id>
		<property name="adminUsername" column="username"/>
		<property name="adminPassword" column="password"/>
		<property name="emailId" column="email_id"/>
		<property name="adminCreatedAt" column="created_at"/>
	</class>
</hibernate-mapping>